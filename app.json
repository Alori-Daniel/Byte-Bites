{
  "expo": {
    "name": "byte-bites",
    "slug": "byte-bites",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bytelogo.png",
    "userInterfaceStyle": "light",
    "scheme": "bytebites",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.its.bites"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/bytelogo.png",
        "backgroundImage": "./assets/images/bytelogo.png",
        "monochromeImage": "./assets/images/bytelogo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.its.bites",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/bytelogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/bytelogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.754212586406-q32mst6v1a3mi2lld8c45bek27pmvc24"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e61a7731-b25d-4470-978f-045ba909cc85"
      }
    },
    "owner": "itsalori"
  }
}
